.mypage-main-container
  = render 'products/header'
  - breadcrumb :user_sign_out
  = render 'mypage/bread-crumbs'
  %main.mypage-container.clearfix
    = render 'mypage/side-bar'
    .mypage-content
      .mypage-content__chapter-container
        .signup-main__inner
          .signup-main__content
            = link_to destroy_user_session_path, method: 'delete', class: 'signup-main__btn-default signup-main__btn-red', id: "logout-btn" do
              ログアウト
  = render 'products/footer'
  = render 'products/sellbuttom'
